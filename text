import math

x,xo,x1,x2=3,2,4,5

def e(x):
    y=math.exp(x)
    return y

def interpolacion_cuadratica():
    b0 = e(xo)
    b1 = (e(x1)-e(xo))/(x1-x0)
    b2 = (((e(x2)-e(x1))/(x2-x1))-b1)/(x2-x0)
    
    fo = b0+b1*(x-x0)+b2*(x-x0)*(x-x1)
    return fo

def error(x):
    Et = (math.exp(3)-x)/math.exp(3)*100
    return Et

print("e^3 por interpolacion cuadratica :",interpolacion_cuadratica())
print("Error: ", error(interpolacion_cuadratica()))
